  System.SysUtils, System.Classes, Vcl.Controls, Vcl.StdCtrls;

type
  TEditCep = class(TEdit)
  private
    FButton: TButton;
    { Private declarations }
  protected
    { Protected declarations }
  public
    constructor Create(AOwner: TComponent); override;
    { Public declarations }
  published
    { Published declarations }
  end;

procedure Register;

implementation

